{
  "name": "javascript-graphql-server",
  "version": "1.0.0",
  "description": "Javascript GraphQL Server",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, css, json}": [
      "eslint src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "bcrypt": "^3.0.1",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.2.1",
    "express-session": "^1.15.6",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.1",
    "ioredis": "^4.0.2",
    "merge-graphql-schemas": "^1.5.7",
    "pg": "^7.5.0",
    "rate-limit-redis": "^1.5.0",
    "sequelize": "^4.39.0",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3"
  },
  "author": "Terry Tilley <terry@terrytilley.com>",
  "license": "ISC"
}
